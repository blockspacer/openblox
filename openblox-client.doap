<Project xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:gnome="http://api.gnome.org/doap-extensions#"
	xmlns="http://usefulinc.com/ns/doap#">

	<name xml:lang="en">OpenBlox Client</name>

	<shortdesc xml:lang="en">
	The reference client implementation for the OpenBlox game engine.
	</shortdesc>

	<description xml:lang="en">
	An open source game engine, with a Lua API similar to that of ROBLOX.
	</description>

	<homepage rdf:resource="https://gitlab.com/openblox/openblox" />
	<license rdf:resource="http://www.gnu.org/licenses/gpl-3.0.html"/>
	<bug-database rdf:resource="https://myzillawr.tk/bugzilla/" />

	<category rdf:resource="http://api.gnome.org/doap-extensions#apps" />
	<programming-language>C++</programming-language>

	<maintainer>
		<foaf:Person>
			<foaf:name>John M. Harris, Jr.</foaf:name>
			<foaf:mbox rdf:resource="mailto:johnmh@openmailbox.org" />
			<foaf:homepage rdf:resource="https://johnmh.me/" />
		</foaf:Person>
	</maintainer>

	<repository>
		<GitRepository>
			<location rdf:resource="https://gitlab.com/openblox/openblox-client.git"/>
			<browse rdf:resource="https://gitlab.com/openblox/openblox-client"/>
		</GitRepository>
	</repository>
</Project>
